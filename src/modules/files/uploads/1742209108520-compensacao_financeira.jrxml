	</parameter>
	<parameter name="CODCONTA" class="java.lang.String">
		<property name="PESQUISA" value="TRUE"/>
		<property name="NOMETABELA" value="AD_CONTAPARCEIRO"/>
		<parameterDescription><![CDATA[Conta Parceiro]]></parameterDescription>
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT

CTA.CODCONTA,
EXT.NUFIN,
EXT.DATABAIXA,
EXT.VLRBAIXA,
EXT.NUCOMPENS,
EXT.NUMNOTA,
PARC.NOMEPARC,
Formatar_Cpf_Cnpj(PARC.CGC_CPF) AS CNPJ,

TRUNC(FIN.DTVENC) AS VENCIMENTO,
FIN.VLRDESDOB,
FIN.VLRMULTA,
FIN.VLRJURO,
NVL((SELECT SUM(VLRBAIXA) AS SALDO_INICIAL FROM AD_EXTRATOPARCEIRO WHERE CODCONTA = CTA.CODCONTA AND DATABAIXA < $P{DATAINI}),0) AS SALDO



FROM
AD_EXTRATOPARCEIRO EXT
INNER JOIN AD_CONTAPARCEIRO CTA ON EXT.CODCONTA = CTA.CODCONTA
INNER JOIN TGFPAR PARC ON CTA.CODPARC = PARC.CODPARC
INNER JOIN TGFFIN FIN ON EXT.NUFIN = FIN.NUFIN

WHERE (CTA.CODCONTA = $P{CODCONTA} OR $P{CODCONTA} IS NULL)

AND EXT.DATABAIXA >= $P{DATAINI} AND EXT.DATABAIXA <= $P{DATAFIM}

AND CTA.TIPO = $P{TIPO}

ORDER BY CTA.CODCONTA ASC,  EXT.DATABAIXA ASC]]>
	</queryString>
